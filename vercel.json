
{
  "functions": {
    "api/change-birthdate.js": {
      "maxDuration": 30
    },
    "api/get-csrf.js": {
      "maxDuration": 30
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/public/index.html"
